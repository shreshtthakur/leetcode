int pivotIndex(vector<int>& nums) {
        int i=0,ls=0,rs=0,j=nums.size()-1;
        while(i<j){
            ls+=nums[i];
            rs+=nums[j];
            cout<<ls<<" "<<rs<<endl;
            if(ls==rs){
                return i++;
            }
            i++;
            j--;
        }
        return -1;
    }
